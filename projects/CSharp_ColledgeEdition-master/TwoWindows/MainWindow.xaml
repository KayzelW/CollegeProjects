<Window x:Class="TwoWindows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwoWindows"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Name="mainWindow">
    <Grid>
        <StackPanel>
            <TextBlock Text="MainWindow" VerticalAlignment="Center" HorizontalAlignment="Center"></TextBlock>
            <Button Content="Open SupWindow" Click="Button_Click" HorizontalAlignment="Center"></Button>
        </StackPanel>
    </Grid>
</Window>
