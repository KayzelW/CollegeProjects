<Window x:Class="TwoWindows.SupWindowResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwoWindows"
        mc:Ignorable="d"
        Title="Результат" Height="450" Width="400" Name="supWindowResult">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Column="0" Grid.Row="0">
            <Label Content="Error" HorizontalAlignment="Left" Width="Auto" Margin="20, 50" FontSize="20" Foreground="Red" Name="errorLabel"></Label>
        </StackPanel>
        <StackPanel Grid.Column="0" Grid.Row="1">
            <TextBlock Text="Пояснение" HorizontalAlignment="Center" Width="Auto" TextAlignment="Left" Margin="0, 5, 0, 5" FontSize="20"></TextBlock>
            <TextBlock Text="resultBox" HorizontalAlignment="Left" Width="Auto" TextAlignment="Left" Margin="20, 5" Name="resShow" Visibility="Hidden"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
